
echo "forward loop";
for i in 0:10 {
    print(i);
}

echo "reverse loop";
for i in 5 + 5 : 0 {
    print(i);
}

echo "expression in range";
fn ten() { 10 }
for i in 5 - 5 : ten() {
    print(i);
}

echo "implicit start value";
for i in 10 {
    print(i);
}

echo "inclusive end value";
for i in 1:=10 {
    print(i);
}

echo "inclusive end value, reverse";
for i in 10:=0 {
    print(i);
}
